/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2020",
    "module": "es2020",
    "jsx": "react-jsx",
    "lib": [
      "es2020",
      "dom"
    ],
    "useDefineForClassFields": false,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*"
  ]
}
